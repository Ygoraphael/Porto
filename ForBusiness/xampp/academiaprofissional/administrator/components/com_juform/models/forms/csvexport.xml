<?xml version="1.0" encoding="utf-8" ?>
<form addrulepath="/administrator/components/com_juform/models/rules">
    <fieldset name="details"
              addfieldpath="/administrator/components/com_juform/models/fields">
        <field
                name="csv_export_file_name"
                type="text"
                size="32"
                required="true"
                default=""
                label="COM_JUFORM_FIELD_EXPORTED_FILE_NAME_LABEL"
                description="COM_JUFORM_FIELD_EXPORTED_FILE_NAME_DESC"
                />

        <field
                name="csv_created_from_filter"
                type="calendar"
                default=""
                format="%Y-%m-%d %H:%M:%S"
                label="COM_JUFORM_FIELD_CREATED_FROM_FILTER_LABEL"
                description="COM_JUFORM_FIELD_CREATED_FROM_FILTER_DESC"
                />

        <field
                name="csv_created_to_filter"
                type="calendar"
                default=""
                format="%Y-%m-%d %H:%M:%S"
                label="COM_JUFORM_FIELD_CREATED_TO_FILTER_LABEL"
                description="COM_JUFORM_FIELD_CREATED_TO_FILTER_DESC"
                />

        <field
                name="csv_limit_export"
                type="text"
                default="100"
                label="COM_JUFORM_FIELD_EXPORT_LIMIT_LABEL"
                description="COM_JUFORM_FIELD_EXPORT_LIMIT_DESC"
                />
    </fieldset>
</form>
